<section>
  <h1 class="h3 mb-3 text-gray-800">Deployments</h1>
</section>
<section>
  <div class="row">
    <div class="col-lg-9">
      <div class="card">
        <div class="card-body">
          <div class="table-responsive">
            <table class="table table-sm table-bordered">
              <thead class="thead-dark">
                <tr>
                  <th scope="col">ID</th>
                  <th scope="col">Status</th>
                  <th scope="col">Website id</th>
                  <th scope="col">Created at</th>
                  <th scope="col">Updated at</th>
                  <th scope="col"></th>
                </tr>
              </thead>
              <tbody>              
              <% @deployments.each do |d| %>
                <tr>
                  <td><%= link_to d['id'], "/admin/instances/#{d['website_id']}/access/deployments/#{d['id']}" %></td>
                  <td><%= d['website_id'] %></td>
                  <td><%= d['status'] %></td>
                  <td><%= d['created_at'] %></td>
                  <td><%= d['updated_at'] %></td>
                  <td>
                    <div class="d-flex">
                      <ul>
                        
                      </ul>
                    </div>
                  </td>
                </tr>
              <% end %>     
              </tbody>
            </table>           
          </div>
        </div>
      </div>
    </div>
    <div class="col-lg-3">
      <%= render 'super_admin/shared/sidebar' %>
    </div>    
  </div>
</section>